.banner {
  position: relative;
  margin: 0;
  background-image: linear-gradient(-180deg, rgba(234, 82, 111, 0.33) 7%, rgba(244, 244, 244, 0.23) 38% ), url('<%= asset_path("index-banner.jpg") %>');
  background-size: cover !important;
  background-position: top;
  text-align:center;
  color: white;
  text-align: center;
  height: 70vh;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
}
